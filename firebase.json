
{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "nextjsServer"
      }
    ]
  },
  "functions": {
    "source": ".",
    "runtime": "nodejs16",
    "ignore": [
      "firebase.json",
      ".firebaserc",
      ".firebase/**",
      "**/node_modules/**",
      "public/**"
    ]
  }
}